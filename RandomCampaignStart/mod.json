    "NotRandomMode": false, //If TagRandomLance set to true this toggle changes nothing.

    /*TagRandomLance: true to have Starting Lance consist of local Mech variants; false to allow one of the other methods: FullRandom or LegacyRandom*/
    "TagRandomLance": false,

    /* Global toggles that effect all types of randomizers.*/
    "MinimumStartingWeight": 160,
    "MaximumStartingWeight": 170,
    "NumberRandomRonin": 3,
    "NumberProceduralPilots": 0,
    "MechPercentageStartingCost": 1,
    "IgnoreAncestralMech": false,

